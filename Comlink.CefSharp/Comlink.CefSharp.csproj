<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>latest</LangVersion>

    <Authors>Chris Kruining</Authors>
    <PackageId>Comlink.CefSharp</PackageId>
    <Copyright>MIT</Copyright>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>

    <PackageProjectUrl>https://github.com/chris-kruining/comlink-sharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/chris-kruining/comlink-sharp</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>

    <Version>0.0.11</Version>
    <AssemblyVersion>0.0.11</AssemblyVersion>
    <FileVersion>0.0.11</FileVersion>

    <Platforms>x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Comlink.Core\Comlink.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CefSharp.Wpf.NETCore" Version="116.0.190" />
  </ItemGroup>

</Project>
